{
	"restartable": "rs",
	"ignore": [
		".git",
		".gitattributes",
		".gitignore",
		"logs/",
		"libs/views",
		"libs/public",
		"node_modules/**/node_modules"
	],
	"verbose": true,
	"execMap": {
		"js": "node --harmony"
	},
	"watch": [
		"libs/"
	],
	"env": {
		"NODE_ENV": "development"
	},
	"ext": "js json"
}
